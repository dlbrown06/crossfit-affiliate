{
  "parser": "babel-eslint",
  "extends": [
    "prettier"
  ],

  "plugins": [
    "prettier",
    "react"
  ],

  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  },

  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}